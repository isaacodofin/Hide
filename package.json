{
  "name": "GIFT-MD",
  "version": "4.2.0",
  "description": "THE BEST BOT IN THE UNIVERSE",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">= 20.x",
    "npm": ">= 10.x"
  },
"scripts": {
    "start": "node --expose-gc --max-old-space-size=280 --optimize-for-size --gc-interval=100 index.js",
    "dev": "nodemon --expose-gc --max-old-space-size=280 --optimize-for-size --gc-interval=100 index.js",
    "start:safe": "node --expose-gc --max-old-space-size=320 index.js",
    "start:aggressive": "node --expose-gc --max-old-space-size=280 --optimize-for-size --gc-interval=50 --max-semi-space-size=8 index.js",
    "cleanup": "node cleanup.js",
    "reset-session": "rm -rf data/session/auth.db && echo 'Session deleted'",
    "start:clean": "npm run cleanup && npm start",
    "start:fresh": "npm run reset-session && npm start",
    "logs": "tail -f logs/*.log 2>/dev/null || echo 'No logs found'",
    "watch": "nodemon --exec node --expose-gc --max-old-space-size=280 index.js"
  },
  "nodemonConfig": {
    "ignore": ["data/*", "*.json", "logs/*"],
    "delay": "2000",
    "execMap": {
      "js": "node --expose-gc --max-old-space-size=280"
    }
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.19",
    "better-sqlite3": "^12.2.0",
    "awesome-phonenumber": "^5.9.0",
    "axios": "^1.8.4",
    "adm-zip": "^0.5.16",
    "@napi-rs/canvas": "^0.1.53",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "dotenv": "^16.4.5",
    "events": "^3.3.0",
    "file-type": "^16.5.4",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "fs-extra": "^11.2.0",
    "gtts": "^0.2.1",
    "gradient-string": "^2.0.2",
    "human-readable": "^0.2.1",
    "jimp": "^1.6.0",
    "jsdom": "^22.1.0",
    "libphonenumber-js": "^1.11.18",
    "libsignal": "^2.0.1",
    "link-preview-js": "^3.0.5",
    "moment-timezone": "^0.5.43",
    "mumaker": "^2.0.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "node-id3": "^0.2.3",
    "node-webpmux": "^3.1.0",
    "node-youtube-music": "^0.8.3",
    "performance-now": "^2.1.0",
    "phin": "^3.7.1",
    "pino": "^8.21.0",
    "sqlite3": "^5.1.7",
    "sqlite": "^5.1.1",
    "pm2": "^6.0.8",
    "pdfkit": "^0.15.0",
    "qrcode": "^1.5.4",
    "qrcode-reader": "^1.0.4",
    "qrcode-terminal": "^0.12.0",
    "request": "^2.88.2",
    "ruhend-scraper": "^8.3.0",
    "safe-stable-stringify": "^2.5.0",
    "set-cookie": "^0.0.4",
    "sharp": "^0.32.6",
    "tough-cookie": "^5.0.0",
    "translate-google-api": "^1.0.4",
    "ws": "^8.17.1",
    "yargs": "^17.6.0",
    "yargs-parser": "^21.1.1",
    "youtube-yts": "^2.0.0",
    "youtubedl-core": "^4.11.7",
    "yt-search": "^2.12.1",
    "ytdl-core": "^4.11.5"
  }
}
